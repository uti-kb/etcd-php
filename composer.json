{
  "name": "uti-kb/etcd-php",
  "type": "library",
  "description": "Fork of PHP client for Etcd v3",
  "keywords": ["etcdv3", "etcd", "etcd-client", "etcd-php"],
  "homepage": "https://github.com/uti-kb/etcd-php",
  "license": "MIT",
  "authors": [
    {
      "name": "ouqiang",
      "email": "qingqianludao@gmail.com",
      "homepage": "https://github.com/ouqiang"
    },
    {
      "name": "David Pauli",
      "email": "info@blackpanda.media",
      "homepage": "https://www.david-pauli.de"
    }
  ],
   "require": {
     "php": "7.4.* || 8.0.* || 8.1.* || 8.2.*",
     "ext-json": "*",
     "guzzlehttp/guzzle": "^7.7.0"
   },
   "require-dev": {
     "friendsofphp/php-cs-fixer": "3.*",
     "phpmd/phpmd": "2.*",
     "phpstan/phpstan": "1.*",
     "phpunit/phpunit": "9.*",
     "roave/security-advisories": "dev-master",
     "squizlabs/php_codesniffer": "3.*"
   },
   "autoload": {
     "psr-4": {
       "BPM\\Etcd\\": "src/"
     }
   },
   "autoload-dev": {
     "psr-4": {
       "BPM\\Etcd\\Tests\\": "tests/"
     }
   }
}
